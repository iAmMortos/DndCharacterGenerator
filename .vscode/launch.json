	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Current File",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"cwd": "${workspaceFolder}/${relativeFileDirname}",
			"console": "integratedTerminal",
			"windows": {
				"cwd": "${workspaceFolder}\\${relativeFileDirname}"
			}
		},
		{
			"name": "Python: Run Data Tests",
			"type": "python",
			"request": "launch",
			"program": "runner.py",
			"console":"integratedTerminal",
			"cwd": "${workspaceFolder}/test/data_tests",
			"windows": {
				"cwd": "${workspaceFolder}\\test\\data_tests",
			}
		},
	]
}